<%-include("partials/header")%>  
<h1>Home</h1>
<p><%= starting %> </p>
<% post_array.forEach(function (elements){%>
    <h1><%= elements.title %></h1>
    <%const content = elements.content.substring(0,100)+"...";%>
    <%if(content.length>100){%>
    <p><%= content %>
    <a href="/posts/<%=elements.title%>">Read More</a>
    </p>
    <%}%>
    <%})%>


<%-include("partials/footer")%>